<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Icons/Icons.xaml"/>
    </ResourceDictionary.MergedDictionaries>


    <DrawingBrush x:Key="LogoLightBrush" Drawing="{StaticResource ViLogoLight}" />
    <DrawingBrush x:Key="LogoDarkBrush" Drawing="{StaticResource ViLogoDark}" />

    <DrawingBrush x:Key="LoginCircleLogoLightBrush" Drawing="{StaticResource LoginCircleLogoLight}" />
    <DrawingBrush x:Key="LoginCircleLogoDarkBrush" Drawing="{StaticResource LoginCircleLogoDark}" />
    <DrawingBrush x:Key="LoginQRCircleLightBrush" Drawing="{StaticResource LoginQRCircleLight}" />

</ResourceDictionary>
